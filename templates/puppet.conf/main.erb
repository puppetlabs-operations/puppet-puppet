[main]
server        = <%= @server %>
ca_server     = <%= @ca_server %>
report_server = <%= @report_server %>
pluginsync    = true

logdir = <%= @puppet_logdir %>
vardir = <%= @puppet_vardir %>
ssldir = <%= @puppet_ssldir %>
rundir = <%= @puppet_rundir %>

<% if report_format -%>
report_serialization_format = <%= report_format %>
<% end -%>
